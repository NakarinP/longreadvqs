#Plot box-plots of three VQS metrics variation through different sampling depths.
divplots <- function(data = dataframe, depthorder = depthorder, nametitle = "virus"){
haplotypes <- ggplot(data, aes(x = factor(depth, level=depthorder), y = haplotypes,
group = interaction(depth, sample), fill = sample, color = sample, alpha = 0.2)) + geom_boxplot() +
xlab("depth") + theme(legend.position = "none") + ylab("H") +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1))
shannon <- ggplot(data, aes(x = factor(depth, level=depthorder), y = shannon,
group = interaction(depth, sample), fill = sample, color = sample, alpha = 0.2)) + geom_boxplot() +
xlab("depth") + theme(legend.position = "none") + ylab(bquote(H[S])) +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1))
norm_shannon <- ggplot(data, aes(x = factor(depth, level=depthorder), y = norm_shannon,
group = interaction(depth, sample), fill = sample, color = sample, alpha = 0.2)) + geom_boxplot() +
xlab("depth") + theme(legend.position = "none") + ylab(bquote(H[SN])) +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1))
gini_simpson <- ggplot(data, aes(x = factor(depth, level=depthorder), y = gini_simpson,
group = interaction(depth, sample), fill = sample, color = sample, alpha = 0.2)) + geom_boxplot() +
xlab("depth") + theme(legend.position = "none") + ylab(bquote(H[GS])) +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1))
FAD <- ggplot(data, aes(x = factor(depth, level=depthorder), y = FAD,
group = interaction(depth, sample), fill = sample, color = sample, alpha = 0.2)) + geom_boxplot() +
xlab("depth") + theme(legend.position = "none") +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1))
Mfe <- ggplot(data, aes(x = factor(depth, level=depthorder), y = Mfe,
group = interaction(depth, sample), fill = sample, color = sample, alpha = 0.2)) + geom_boxplot() +
xlab("depth") + theme(legend.position = "none") +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1))
Pie <- ggplot(data, aes(x = factor(depth, level=depthorder), y = Pie,
group = interaction(depth, sample), fill = sample, color = sample, alpha = 0.2)) + geom_boxplot() +
xlab("depth") + theme(legend.position = "none") + ylab(bquote(π[e])) +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1))
Mfm <- ggplot(data, aes(x = factor(depth, level=depthorder), y = Mfm,
group = interaction(depth, sample), fill = sample, color = sample, alpha = 0.2)) + geom_boxplot() +
xlab("depth") + theme(legend.position = "none") +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1))
Pim <- ggplot(data, aes(x = factor(depth, level=depthorder), y = Pim,
group = interaction(depth, sample), fill = sample, color = sample, alpha = 0.2)) + geom_boxplot() +
xlab("depth") + theme(legend.position = "none") + ylab(bquote(π[m])) +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1))
LG <- ggplot(data, aes(x = factor(depth, level=depthorder), y = Pim,
group = interaction(depth, sample), fill = sample, color = sample), alpha = 0.2) + geom_boxplot() +
xlab("depth") + theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1))
allplot <- plot_grid(haplotypes, shannon, norm_shannon, gini_simpson, FAD, Mfe, Pie, Mfm, Pim, nrow = 3, align = "v")
lg <- get_legend(LG, position = NULL)
sumplot <- plot_grid(allplot, lg, rel_widths = c(4, .4))
sumplot2 <- sumplot + draw_figure_label(label = nametitle, position = "bottom.right", fontface = "bold", size = 20)
return(sumplot2)
}
