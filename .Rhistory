gs <- function(data = dataframe, depthorder = depthorder, nametitle = "virus"){
a <- ggplot(data, aes(x = factor(depth, level=depthorder), y = gini_simpson,
group = interaction(depth, sample), fill = sample, color = sample, alpha = 0.2)) + geom_boxplot() +
xlab("depth") + ylab(bquote(H[GS])) + theme(legend.position = "bottom", legend.box = "horizontal") + scale_alpha(guide = 'none') +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1)) + facet_wrap(~samplingwhen, nrow = 2) + ggtitle(nametitle) + theme(legend.title=element_blank())
return(a)
}
